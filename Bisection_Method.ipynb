      "source": [
        "\n",
        "def f(x):\n",
        "    return (Expression(x))\n",
        "\n",
        "def bisection_method(f,a, b, tol):\n",
        "    n = 1\n",
        "    tab = np.empty((0,5), dtype=float)\n",
        "   \n",
        "\n",
        "  \n",
        "    if f(a)*f(b) > 0:\n",
        "        #end function, no root.\n",
        "        print(\"No root found.\")\n",
        "\n",
        "    else:\n",
        "        while (b - a)/2.0 > tol:\n",
        "            \n",
        "            midpoint = (a + b)/2.0\n",
        "            fa = f(a)\n",
        "            fb = f(b)\n",
        "\n",
        "            fx = float(f((round(midpoint,4)))) \n",
        "            fx_fix = round(fx,4)\n",
        "            tab = np.append(tab,np.array([[n,round(b,4),round(a,4),round(midpoint,4),('{0:.4f}'.format(fx_fix))]]), axis =0, )\n",
        "\n",
        "            if f(midpoint) == 0:\n",
        "                n=n+1\n",
        "                return(midpoint) #The midpoint is the x-intercept/root.\n",
        "            elif fa*f(midpoint) < 0: # Increasing but below 0 case\n",
        "                b = midpoint\n",
        "                n=n+1\n",
        "\n",
        "            else:\n",
        "                a = midpoint\n",
        "                n=n+1\n",
        "\n",
        "        \n",
        "        return(midpoint,n,tab)\n",
        "\n",
        "def bisec(equ,b, a ):\n",
        "  fn = f(equ)\n",
        "  answer,n, tab= bisection_method(fn,b, a, 0.00001)\n",
        "  print(\"N:\",n, \"Answer: \",  round(answer, 4))\n",
        "  df = pd.DataFrame(data=tab, columns=[\"N\", \"A (-ve)\", \"B (+ve)\", \"MidPoint\", \"f(x)\"])\n",
        "  df.index = np.arange(1,len(df)+1)\n",
        "  df\n",
        "  return tab\n",
